+#define REG( addr )  *( (volatile unsigned int *)( addr ) )
+
+static char i2a( int val )
+{
+	if ( val < 10 )
+		return '0'+val;
+	else
+		return 'a'+(val-10);
+}
+
+static char *int_to_hex8( char *dst, int val )
+{
+	int cnt;
+	char *ret;
+
+	ret = dst;
+	dst += 7;
+	for ( cnt = 0 ; cnt < 8 ; cnt++ ) {
+		*dst-- = i2a( val & 0x0f );
+		val >>= 4;
+	}
+	return ret;
+}
+
+static void display_cd( char *title, int pio_mode )
+{
+	char buf[ 32 ];
+	int  cnt;
+	struct pio_desc cd_pin[] = {
+		{"SDMMC1_CD", AT91C_PIN_PA(30), 0, pio_mode, PIO_INPUT},
+		{(char *)0, 0, 0, PIO_DEFAULT, PIO_INPUT},
+	};
+
+	pmc_sam9x5_enable_periph_clk(AT91C_ID_PIOA);
+	pio_configure(cd_pin);
+	usart_puts( title );
+	strcpy( buf, "00000000 00000000 00000000\n" );
+	int_to_hex8( buf, REG( AT91C_BASE_PIOA ) );
+	int_to_hex8( buf + 9, REG( AT91C_BASE_PIOA + 4 ) );
+	int_to_hex8( buf + 18, REG( AT91C_BASE_PIOA + 8 ) );
+	usart_puts( buf );
+	usart_puts( "PIO_PDSRA: " );
+	strcpy( buf, "0x00000000" );
+
+	for ( cnt = 0 ; cnt < 15 ; cnt++ ) {
+		int_to_hex8( buf + 2, REG( AT91C_BASE_PIOA + 8 ) );
+		usart_puts( buf );
+		mdelay( 200 );
+		usart_puts( "\033[10D" );
+	};
+	usart_puts( "\n" );
+}
+
+
+static void display_cd_loop( )
+{
+	do {
+		display_cd( "PULL UP\n", PIO_PULLUP );
+		usart_puts( "\033[3A" );
+		display_cd( "PULL DOWN\n", PIO_PULLDOWN );
+		usart_puts( "\033[3A" );
+		display_cd( "NO PULL\n", PIO_DEFAULT );
+		usart_puts( "\033[3A" );
+	} while( 0 );
+}
